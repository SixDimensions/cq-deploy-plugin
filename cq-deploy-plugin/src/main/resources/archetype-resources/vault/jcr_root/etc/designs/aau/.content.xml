#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <css/>
    <jcr:content
        cq:doctype="html_5"
        jcr:primaryType="nt:unstructured"
        jcr:title="Academy of Art University Site"
        sling:resourceType="wcm/core/components/designer">
        <session jcr:primaryType="nt:unstructured">
            <session jcr:primaryType="nt:unstructured">
                <par
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="foundation/components/parsys"
                    components="[/libs/foundation/components/text,/apps/${parentArtifactId}/components/components/accordian,/apps/${parentArtifactId}/components/components/assignments,/apps/${parentArtifactId}/components/components/childlist,/apps/${parentArtifactId}/components/components/slideshow,/apps/${parentArtifactId}/components/components/video]">
                    <section jcr:primaryType="nt:unstructured"/>
                    <accordian jcr:primaryType="nt:unstructured">
                        <accordian-items
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="foundation/components/parsys"
                            components="/apps/${parentArtifactId}/components/components/accordian-item">
                            <section jcr:primaryType="nt:unstructured"/>
                        </accordian-items>
                    </accordian>
                </par>
            </session>
        </session>
        <page/>
    </jcr:content>
    <images/>
    <ui.widgets.css/>
    <favicon.ico/>
    <clientlibs/>
    <resources/>
    <static.css/>
    <dev/>
</jcr:root>
